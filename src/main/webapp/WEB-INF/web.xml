<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://JAVA.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Servlets Assignment Q2 Q3 Q4 and Q5</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
   <servlet-name>s1</servlet-name>
  <servlet-class>com.srivani.LoginServlet</servlet-class>
  <init-param>
  <param-name>name</param-name>
  <param-value>mani</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>s1</servlet-name>
  <url-pattern>/login</url-pattern>
  
    </servlet-mapping>
    
    
    <filter>
  <filter-name>filter1</filter-name>
  <filter-class>com.srivani.LogFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>filter1</filter-name>
  <url-pattern>/login</url-pattern>
  </filter-mapping>
  
  <servlet>
   <servlet-name>s2</servlet-name>
  <servlet-class>com.srivani.HomeServlet</servlet-class>
  <init-param>
  <param-name>name</param-name>
  <param-value>Srivani</param-value>
  </init-param>
  </servlet>
  <servlet-mapping>
  <servlet-name>s2</servlet-name>
  <url-pattern>/home</url-pattern>
  
    </servlet-mapping>
  
  <context-param>
  <param-name>name</param-name>
  <param-value>Srivani Assignment</param-value>
  </context-param>
  
  <servlet>
   <servlet-name>s3</servlet-name>
  <servlet-class>com.srivani.Youtube</servlet-class>
  
  </servlet>
  <servlet-mapping>
  <servlet-name>s3</servlet-name>
  <url-pattern>/red</url-pattern>
  
    </servlet-mapping>
  
</web-app>